{
  "version": "2.0",
  "logging": {
    "logLevel": {
      "Host": "Warning",
      "Function": "Information",
      "default": "Warning"
    },
    "console": {
      "isEnabled": true,
      "DisableColors": false
    }
  },
  "extensionBundle": {
    "id": "Microsoft.Azure.Functions.ExtensionBundle",
    "version": "[4.*, 5.0.0)"
  },
  "extensions": {
    "durableTask": {
      "hubName": "AIChatHub",
      "storageProvider": {
        "connectionStringName": "AzureWebJobsStorage"
      }
    }
  }
}